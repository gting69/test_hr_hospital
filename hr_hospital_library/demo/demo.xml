<odoo>
    <data noupdate="1">
        <record id="spec_cardiology" model="doctor.specialty"><field name="name">Cardiology</field><field name="code">CARD-01</field></record>
        <record id="spec_pediatrics" model="doctor.specialty"><field name="name">Pediatrics</field><field name="code">PEDI-02</field></record>
        <record id="spec_surgery" model="doctor.specialty"><field name="name">Surgery</field><field name="code">SURG-03</field></record>
        <record id="spec_therapy" model="doctor.specialty"><field name="name">General Therapy</field><field name="code">THER-04</field></record>
        <record id="spec_neurology" model="doctor.specialty"><field name="name">Neurology</field><field name="code">NEUR-05</field></record>

        <record id="dis_infect" model="hr.hospital.disease.type"><field name="name">Infectious</field><field name="severity_level">high</field><field name="is_contagious">True</field></record>
        <record id="dis_covid" model="hr.hospital.disease.type"><field name="name">COVID-19</field><field name="parent_id" ref="dis_infect"/><field name="icd_10_code">U07.1</field><field name="severity_level">critical</field><field name="country_ids" eval="[(4, ref('base.ua')), (4, ref('base.us'))]"/></record>
        <record id="dis_flu" model="hr.hospital.disease.type"><field name="name">Influenza</field><field name="parent_id" ref="dis_infect"/><field name="icd_10_code">J11</field><field name="severity_level">medium</field></record>
        <record id="dis_cardio" model="hr.hospital.disease.type"><field name="name">Cardiovascular</field><field name="severity_level">high</field></record>
        <record id="dis_hyper" model="hr.hospital.disease.type"><field name="name">Hypertension</field><field name="parent_id" ref="dis_cardio"/><field name="icd_10_code">I10</field></record>
        <record id="dis_heart_fail" model="hr.hospital.disease.type"><field name="name">Heart Failure</field><field name="parent_id" ref="dis_cardio"/><field name="icd_10_code">I50</field></record>
        <record id="dis_resp" model="hr.hospital.disease.type"><field name="name">Respiratory</field><field name="severity_level">medium</field></record>
        <record id="dis_asthma" model="hr.hospital.disease.type"><field name="name">Asthma</field><field name="parent_id" ref="dis_resp"/><field name="icd_10_code">J45</field></record>
        <record id="dis_pneumo" model="hr.hospital.disease.type"><field name="name">Pneumonia</field><field name="parent_id" ref="dis_resp"/><field name="icd_10_code">J18</field></record>
        <record id="dis_neuro" model="hr.hospital.disease.type"><field name="name">Neurological</field><field name="severity_level">high</field></record>
        <record id="dis_migraine" model="hr.hospital.disease.type"><field name="name">Migraine</field><field name="parent_id" ref="dis_neuro"/><field name="icd_10_code">G43</field></record>
        <record id="dis_epilepsy" model="hr.hospital.disease.type"><field name="name">Epilepsy</field><field name="parent_id" ref="dis_neuro"/><field name="icd_10_code">G40</field></record>

        <record id="doc_1" model="hr.hospital.library.doctor"><field name="last_name">Kovalenko</field><field name="first_name">Olena</field><field name="specialty_id" ref="spec_cardiology"/><field name="license_number">L-001</field><field name="user_id" ref="base.user_admin"/></record>
        <record id="doc_2" model="hr.hospital.library.doctor"><field name="last_name">Melnyk</field><field name="first_name">Ivan</field><field name="specialty_id" ref="spec_surgery"/><field name="license_number">L-002</field></record>
        <record id="doc_3" model="hr.hospital.library.doctor"><field name="last_name">Bondar</field><field name="first_name">Mariya</field><field name="specialty_id" ref="spec_pediatrics"/><field name="license_number">L-003</field></record>
        <record id="doc_4" model="hr.hospital.library.doctor"><field name="last_name">Tkach</field><field name="first_name">Petro</field><field name="specialty_id" ref="spec_therapy"/><field name="license_number">L-004</field></record>
        <record id="doc_5" model="hr.hospital.library.doctor"><field name="last_name">Lytvyn</field><field name="first_name">Oleg</field><field name="specialty_id" ref="spec_neurology"/><field name="license_number">L-005</field></record>
        <record id="doc_6" model="hr.hospital.library.doctor"><field name="last_name">Moroz</field><field name="first_name">Anna</field><field name="specialty_id" ref="spec_therapy"/><field name="license_number">L-006</field></record>
        <record id="doc_int_1" model="hr.hospital.library.doctor"><field name="last_name">Intern</field><field name="first_name">Yuriy</field><field name="is_intern">True</field><field name="mentor_id" ref="doc_1"/><field name="license_number">I-001</field></record>
        <record id="doc_int_2" model="hr.hospital.library.doctor"><field name="last_name">Intern</field><field name="first_name">Alina</field><field name="is_intern">True</field><field name="mentor_id" ref="doc_2"/><field name="license_number">I-002</field></record>

        <record id="cont_1" model="contact.person"><field name="last_name">Sydorenko</field><field name="first_name">Natalia</field><field name="relation">Mother</field></record>

        <record id="pat_1" model="hr.hospital.library.patient"><field name="last_name">Shevchenko</field><field name="first_name">Andriy</field><field name="personal_doctor_id" ref="doc_1"/><field name="birth_date">1990-01-01</field></record>
        <record id="pat_2" model="hr.hospital.library.patient"><field name="last_name">Boyko</field><field name="first_name">Ivan</field><field name="personal_doctor_id" ref="doc_2"/><field name="birth_date">1985-05-12</field></record>
        <record id="pat_3" model="hr.hospital.library.patient"><field name="last_name">Kravets</field><field name="first_name">Olga</field><field name="personal_doctor_id" ref="doc_3"/><field name="birth_date">2010-03-20</field><field name="contact_person_id" ref="cont_1"/></record>
        <record id="pat_4" model="hr.hospital.library.patient"><field name="last_name">Kuznetsov</field><field name="first_name">Denys</field><field name="personal_doctor_id" ref="doc_4"/><field name="birth_date">1978-11-30</field></record>
        <record id="pat_5" model="hr.hospital.library.patient"><field name="last_name">Savchenko</field><field name="first_name">Iryna</field><field name="personal_doctor_id" ref="doc_5"/><field name="birth_date">1995-07-15</field></record>
        <record id="pat_6" model="hr.hospital.library.patient"><field name="last_name">Ivanov</field><field name="first_name">Sergiy</field><field name="birth_date">1988-02-10</field></record>
        <record id="pat_7" model="hr.hospital.library.patient"><field name="last_name">Petrenko</field><field name="first_name">Olena</field><field name="birth_date">2001-09-05</field></record>
        <record id="pat_8" model="hr.hospital.library.patient"><field name="last_name">Pavlenko</field><field name="first_name">Dmytro</field><field name="birth_date">1992-12-25</field></record>
        <record id="pat_9" model="hr.hospital.library.patient"><field name="last_name">Lysenko</field><field name="first_name">Tetyana</field><field name="birth_date">1980-04-18</field></record>
        <record id="pat_10" model="hr.hospital.library.patient"><field name="last_name">Rudenko</field><field name="first_name">Vitaliy</field><field name="birth_date">1975-08-22</field></record>
        <record id="pat_11" model="hr.hospital.library.patient"><field name="last_name">Marchenko</field><field name="first_name">Marina</field><field name="birth_date">1998-01-30</field></record>
        <record id="pat_12" model="hr.hospital.library.patient"><field name="last_name">Ponomarenko</field><field name="first_name">Artem</field><field name="birth_date">1983-06-14</field></record>
        <record id="pat_13" model="hr.hospital.library.patient"><field name="last_name">Koval</field><field name="first_name">Oksana</field><field name="birth_date">1994-10-05</field></record>
        <record id="pat_14" model="hr.hospital.library.patient"><field name="last_name">Shpak</field><field name="first_name">Roman</field><field name="birth_date">1987-03-27</field></record>
        <record id="pat_15" model="hr.hospital.library.patient"><field name="last_name">Vovk</field><field name="first_name">Yuliya</field><field name="birth_date">1991-11-11</field></record>

        <record id="vis_1" model="hr.hospital.library.visit"><field name="name">Checkup 1</field><field name="patient_id" ref="pat_1"/><field name="doctor_id" ref="doc_1"/><field name="planned_datetime" eval="datetime.now().strftime('%Y-%m-%d 09:00:00')"/><field name="state">completed</field></record>
        <record id="vis_2" model="hr.hospital.library.visit"><field name="name">Checkup 2</field><field name="patient_id" ref="pat_2"/><field name="doctor_id" ref="doc_2"/><field name="planned_datetime" eval="datetime.now().strftime('%Y-%m-%d 10:00:00')"/><field name="state">completed</field></record>
        <record id="vis_25" model="hr.hospital.library.visit"><field name="name">Urgent Visit</field><field name="patient_id" ref="pat_15"/><field name="doctor_id" ref="doc_4"/><field name="planned_datetime" eval="(datetime.now() + timedelta(days=1)).strftime('%Y-%m-%d 14:00:00')"/><field name="state">planned</field></record>

        <record id="diag_1" model="medical.diagnosis"><field name="visit_id" ref="vis_1"/><field name="disease_id" ref="dis_hyper"/><field name="is_approved">True</field></record>
        <record id="diag_2" model="medical.diagnosis"><field name="visit_id" ref="vis_2"/><field name="disease_id" ref="dis_flu"/><field name="is_approved">True</field></record>
        <record id="diag_20" model="medical.diagnosis"><field name="visit_id" ref="vis_25"/><field name="disease_id" ref="dis_covid"/><field name="is_approved">False</field></record>

        <record id="sched_1" model="doctor.schedule"><field name="doctor_id" ref="doc_1"/><field name="day_of_week">0</field><field name="start_hour">8.0</field><field name="end_hour">16.0</field></record>
        <record id="sched_2" model="doctor.schedule"><field name="doctor_id" ref="doc_2"/><field name="day_of_week">1</field><field name="start_hour">10.0</field><field name="end_hour">18.0</field></record>
        </data>
</odoo>