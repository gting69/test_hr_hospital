<odoo>
    <record
            id="patient_doctor_history_tree"
            model="ir.ui.view">
        <field name="name">patient.doctor.history.tree</field>
        <field name="model">patient.doctor.history</field>
        <field
                name="arch"
                type="xml">
            <tree
                    create="false"
                    edit="false">
                <field
                        name="patient_id"/>
                <field
                        name="doctor_id"/>
                <field
                        name="appointment_date"/>
                <field
                        name="change_date"/>
                <field
                        name="is_active"
                        widget="boolean_toggle"/>
            </tree>
        </field>
    </record>
</odoo>